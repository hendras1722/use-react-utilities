{
  "name": "use-react-utilities",
  "version": "1.0.5-3",
  "description": "A react utilities for need develop your project similar with vue 3",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
   "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.cjs"
    },
    "./components/If": {
      "import": "./lib/esm/components/If.js",
      "require": "./lib/cjs/components/If.cjs"
    },
    "./components/Each": {
      "import": "./lib/esm/components/Each.js",
      "require": "./lib/cjs/components/Each.cjs"
    },
    "./components/Slot": {
      "import": "./lib/esm/components/Slot.js",
      "require": "./lib/cjs/components/Slot.cjs"
    },
    "./components/Component": {
      "import": "./lib/esm/components/Component.js",
      "require": "./lib/cjs/components/Component.cjs"
    }, 
    "./components/KeepAlive": {
      "import": "./lib/esm/components/KeepAlive.js",
      "require": "./lib/cjs/components/KeepAlive.cjs"
    }
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "author": "Muh Syahendra A",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "ofetch": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "http": "^0.0.1-security",
    "react": "workspace:*",
    "react-dom": "workspace:*"
  },
  "workspaces": [
    "packages/vite-project/*"
  ]
}
